set editing-mode vi
set bell-style none
$if mode=vi
    set keymap vi-command
    "gg": beginning-of-history
    "G": end-of-history
    set keymap vi-insert
    "\C-o": " | less"
    "\C-g": " | grep"
    "\C-a": next-history
    "\C-b": previous-history
    "\C-w": backward-kill-word
    "\e.": insert-last-argument
    "\C-l": clear-screen
$endif

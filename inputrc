set editing-mode vi
set bell-style none
Control-o: " | less"
Control-w: backward-kill-word
"\ew": unix-filename-rubout
$if mode=vi
    set keymap vi-command
    "gg": beginning-of-history
    "G": end-of-history
    set keymap vi-insert
    "\e.": insert-last-argument
    "\C-l": clear-screen
$endif
